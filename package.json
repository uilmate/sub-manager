        "test": "jest",
        "build": "rimraf dist && tsc",
        "prepublish": "rimraf dist && yarn test && yarn build"
    },
    "devDependencies": {
        "@types/jest": "^25.2.1",
        "jest": "^25.4.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^25.4.0",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "rxjs": "^6.5.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "testRegex": "/__tests__/.*\\.(ts|js)$",
        "transform": {
            "\\.(ts)$": "ts-jest"
        }
